    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "analyze": "nuxt build --modern=client --analyze"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "nuxt": "^2.12.2"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.10.3",
