{
  "name": "fatec-site",
  "version": "1.0.0",
  "description": "Website da Fatec Taquaritinga constru√≠do com Vue.js e Nuxt.js",
  "author": "Erick Eduardo Petrucelli &lt;erick.petrucelli@fatectq.edu.br&gt;",
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=localhost PORT=3000 nuxt",
    "start": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=80 nuxt start",
    "build": "nuxt build --modern=client",
