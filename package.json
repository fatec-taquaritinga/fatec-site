    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nuxt-webfontloader": "^1.1.0",
    "postcss-color-mod-function": "^3.0.3",
    "prettier": "^1.19.1"
  }
